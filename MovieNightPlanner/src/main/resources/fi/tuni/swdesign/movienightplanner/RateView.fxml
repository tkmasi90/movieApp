<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.Rating?>

<AnchorPane prefHeight="142.0" prefWidth="386.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="fi.tuni.swdesign.movienightplanner.controllers.RateViewController">
    <children>
        <Label layoutX="126.0" layoutY="14.0" text="Rate this movie:">
         <font>
            <Font size="18.0" />
         </font></Label>
        <Rating fx:id="rating" layoutX="14.0" layoutY="40.0" max="10" rating="0.0" />
        <Button fx:id="rateButton" layoutX="169.0" layoutY="93.0" mnemonicParsing="false" onAction="#handleRateButtonAction" text="Rate" />
        <Button fx:id="cancelButton" layoutX="311.0" layoutY="93.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Cancel" />
    </children>
</AnchorPane>
